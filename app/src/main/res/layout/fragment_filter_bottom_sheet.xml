<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/dimen_20"
        tools:context=".presentation.filterBottomSheet.FilterBottomSheetFragment">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/mercedesButton"
            android:layout_width="@dimen/dimen_200"
            android:layout_height="@dimen/dimen_100"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/renaultButton"
            android:layout_width="@dimen/dimen_200"
            android:layout_height="@dimen/dimen_100"
            android:layout_marginTop="@dimen/dimen_100"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mercedesButton" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>